<div class="container" id="users_show_page" style="padding-top: 60px;">
  <h1>Sign Up</h1>
  <%=bootstrap_form_for @user do |f| %>
  <%=f.text_field :name %>
  <%=f.text_field :username %>
  <%=f.text_field :email %>
  <%=f.password_field :password %>
  <%=f.password_field :password_confirmation %>
  <%=f.submit "Sign Up" %>
  <%end%>
</div>